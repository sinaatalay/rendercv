    "OneLineEntry": {
      "additionalProperties": false,
      "description": "This class is the data model of `OneLineEntry`.",
      "properties": {
        "name": {
          "description": "The name of the entry. It will be shown as bold text.",
          "title": "Name",
          "type": "string"
        },
        "details": {
